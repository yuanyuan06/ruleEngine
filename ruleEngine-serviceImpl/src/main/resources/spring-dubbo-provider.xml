<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="rule-provider"/>

    <dubbo:registry protocol="zookeeper" address="zookeeper://zookeeper01.scm.dev.baozun.cn:2181" register="false" />

    <dubbo:protocol name="dubbo" port="20880"/>
    <dubbo:provider protocol="dubbo" default="true" version="${dubbo.default.version}" timeout="350000" />

    <dubbo:service interface="com.engine.service.DemoService" ref="demoService" />
    <dubbo:service interface="com.engine.service.ruleEngine.RuleConfigService" ref="ruleConfigService" />



</beans>